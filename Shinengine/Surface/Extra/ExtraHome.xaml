<Page x:Class="Shinengine.Surface.Extra.ExtraHome"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Shinengine.Surface.Extra"
      mc:Ignorable="d" 
      d:DesignHeight="720" d:DesignWidth="1280"
      Title="ExtraHome">

    <Grid x:Name="ExtraGrid">
        <Grid.Background>
            <ImageBrush ImageSource="/RegularUI/extra.png"/>
        </Grid.Background>
        <Button x:Name="HS1" Height="120" Width="220" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="59,84,0,0" BorderBrush="{x:Null}" Cursor="Hand" Click="Event_HS1" >
            <Button.Background>
                <ImageBrush ImageSource="/RegularUI/Extra/cg01.png"/>
            </Button.Background>
        </Button>
        <Button x:Name="exitlpg" Opacity="1" Style="{StaticResource MaterialDesignRaisedDarkButton}" Width="75" Height="35" ToolTip="Resource name: MaterialDesignRaisedLightDarkButton" HorizontalAlignment="Right" VerticalAlignment="Bottom" Margin="0,0,10,10">
            退出
        </Button>

    </Grid>
</Page>
